<%- include("header"); -%>
  <div class="container">
  <h1 class="mt-5">All Products</h1>
  <ul>
    <%for(let product of products){%>
      <li><a href="/products/<%= product._id %>"><%= product.name %></a></li>
    <%}%>
  </ul>
  <a class="btn btn-primary mt-2" href="/products/new" role="button">Add New Product</a>
  </div>
<%- include("footer"); -%>
